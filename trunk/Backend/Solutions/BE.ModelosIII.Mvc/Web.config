    <add key="webpages:Version" value="2.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="PreserveLoginUrl" value="true" />
    <add key="aspnet:MaxJsonDeserializerMembers" value="600000" />
    <add key="ClientValidationEnabled" value="false" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add key="AllowedMediaTypes" value="image/gif,image/jpeg,image/pjpeg,image/png,image/svg+xml,text/xml" />
    <add key="TempUploadFolder" value="uploads/temp" />
    <add key="PosterUploadFolder" value="uploads/posters" />
    <add key="AllowedRoles" value="Administrador" />
    <add key="ScreeningIntervalInMinutes" value="20" />
